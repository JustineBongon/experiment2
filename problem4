#include<iostream>
#include<conio.h>

using namespace std;

int main()
{
	int a,b,c;
	
	cout << " NUMBER RANKING " << endl;
	cout << " Enter FIRST number ";
	cin >> a;
	cout << " Enter SECOND number ";
	cin >> b;
	cout << " Enter THIRD number ";
	cin >> c;
	
	if (a<b & b<c)
	cout << " 1st: " << c << " 2nd: " << b << " 3rd: " << a;
	else if (b<a & a<c)
	cout << " 1st: " << c << " 2nd: " << a << " 3rd: " << b;
	else if (c<b & b<a)
	cout << " 1st: " << a << " 2nd: " << b << " 3rd: " << c;
	else if (c<a & a<b)
	cout << " 1st: " << b << " 2nd: " << a << " 3rd: " << c;
	else if (b<c & c<a)
	cout << " 1st: " << a << " 2nd: " << c << " 3rd: " << b;
	else if (a<c & c<b)
	cout << " 1st: " << b << " 2nd: " << c << " 3rd: " << a;
	
	_getch();
	return 0;

}
